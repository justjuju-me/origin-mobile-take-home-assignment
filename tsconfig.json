{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "src": ["./src/*"],
      "screens/*": ["./src/screens/*"],
      "components/*": ["./src/components/*"],
      "services/*": ["./src/services/*"],
      "contexts/*": ["./src/contexts/*"],
      "hooks/*": ["./src/hooks/*"],
      "types/*": ["./src/types/*"],
      "shared/*": ["./src/shared/*"]
    }
  }
}
