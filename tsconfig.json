{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "src": ["./src/*"],
      "screens/*": ["./src/screens/*"],
      "components/*": ["./src/components/*"],
      "config/*": ["./src/config/*"],
      "contexts/*": ["./src/contexts/*"],
      "hooks/*": ["./src/hooks/*"],
      "routes/*": ["./src/routes/*"],
      "shared/*": ["./src/shared/*"]
    }
  }
}
